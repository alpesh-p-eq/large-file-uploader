{
    "name": "alpeshequest/large-file-uploader",
    "description": "This is my package large-file-uploader",
    "keywords": [
        "AlpeshEquest",
        "laravel",
        "large-file-uploader"
    ],
    "homepage": "https://github.com/alpeshequest/large-file-uploader",
    "license": "MIT",
    "authors": [
        {
            "name": "Alpesh Patel",
            "email": "alpesh.patel@equestsolutions.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^10.0||^11.0"
    },
    "require-dev": {
        "illuminate/testing": "^10.0",
        "larastan/larastan": "^2.5",
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^7.10.0",
        "orchestra/testbench": "^9.0.0||^8.22.0",
        "pestphp/pest": "^2.5",
        "pestphp/pest-plugin-arch": "^2.1",
        "pestphp/pest-plugin-laravel": "^2.1",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3",
        "spatie/laravel-ray": "^1.35"
    },
    "autoload": {
        "psr-4": {
            "AlpeshEquest\\LargeFileUploader\\": "src/",
            "AlpeshEquest\\LargeFileUploader\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AlpeshEquest\\LargeFileUploader\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint",
        "test-laravel-10": "composer require --dev illuminate/testing:^10.0 && vendor/bin/pest"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AlpeshEquest\\LargeFileUploader\\LargeFileUploaderServiceProvider"
            ],
            "aliases": {
                "LargeFileUploader": "AlpeshEquest\\LargeFileUploader\\Facades\\LargeFileUploader"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
